for i in range(int(input())):
    a,b,c,d=map(int,input().split())
    if a<b:
        a=a+c
        if a>=b:
            b=b+d
        else:
            a=a+d
    else:
        b=b+c
        if a>=b:
            b=b+d
        else:
            a=a+d
    if a>=b:
        print("n")
    else:
        print('s')
